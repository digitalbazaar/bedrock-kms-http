{
  "name": "bedrock-kms-http-test",
  "version": "0.0.1-0",
  "type": "module",
  "description": "Bedrock KMS HTTP API test",
  "private": true,
  "scripts": {
    "test": "npm run test-node",
    "test:debug": "node --preserve-symlinks test.js test --log-level debug",
    "test-node": "node --preserve-symlinks test.js test --framework mocha",
    "test-karma": "node --preserve-symlinks test.js test --framework karma",
    "coverage": "cross-env NODE_ENV=test c8 --reporter=lcov --reporter=text-summary node --preserve-symlinks test.js test --framework mocha",
    "coverage-ci": "cross-env NODE_ENV=test c8 --reporter=lcovonly node --preserve-symlinks test.js test --framework mocha",
    "coverage-report": "c8 report"
  },
  "dependencies": {
    "@bedrock/app-identity": "^4.0.0",
    "@bedrock/core": "^6.0.1",
    "@bedrock/did-context": "^4.0.0",
    "@bedrock/did-io": "^9.0.1",
    "@bedrock/express": "^8.0.0",
    "@bedrock/https-agent": "^4.0.0",
    "@bedrock/jsonld-document-loader": "^3.0.0",
    "@bedrock/karma": "^5.1.0",
    "@bedrock/kms": "^11.0.0",
    "@bedrock/kms-http": "file:..",
    "@bedrock/ledger-context": "^23.0.0",
    "@bedrock/meter": "^4.0.0",
    "@bedrock/meter-http": "^9.0.1",
    "@bedrock/meter-usage-reporter": "^8.0.0",
    "@bedrock/mongodb": "^10.0.0",
    "@bedrock/package-manager": "^3.0.0",
    "@bedrock/security-context": "^7.0.0",
    "@bedrock/server": "^5.0.0",
    "@bedrock/ssm-mongodb": "^10.0.0",
    "@bedrock/test": "^8.0.5",
    "@bedrock/validation": "^7.0.0",
    "@bedrock/veres-one-context": "^14.0.1",
    "@bedrock/zcap-storage": "^8.0.0",
    "@digitalbazaar/did-method-key": "^3.0.0",
    "@digitalbazaar/ed25519-signature-2020": "^4.0.1",
    "@digitalbazaar/ezcap": "^3.0.1",
    "@digitalbazaar/http-client": "^3.2.0",
    "@digitalbazaar/http-signature-zcap-invoke": "^6.0.0",
    "@digitalbazaar/webkms-client": "^11.0.0",
    "@digitalbazaar/zcap": "^8.0.0",
    "base58-universal": "^2.0.0",
    "c8": "^7.11.3",
    "cross-env": "^7.0.3",
    "jsonld-signatures": "^10.0.0",
    "p-map": "^5.5.0",
    "uuid": "^8.3.2"
  },
  "c8": {
    "excludeNodeModules": false,
    "include": [
      "node_modules/@bedrock/kms-http/**"
    ],
    "exclude": [
      "node_modules/@bedrock/kms-http/node_modules/**"
    ]
  }
}
